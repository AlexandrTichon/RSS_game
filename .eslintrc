{
    "extends": "airbnb",
    "env": {
        "node": true,
        "browser": true
    },
    "rules":{
        "linebreak-style": 0,
        "no-param-reassign": [2, { 
            "props": false
        }],
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "parser": "babel-eslint"
  }
